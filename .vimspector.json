{
  "configurations": {
    "Python": {
      "adapter": "debugpy",
      "filetypes": [ "python" ],
      "default": true,
      "configuration": {
        "request": "launch",
        "program": "gunicorn --bind 0.0.0.0:5000 mainwebui:app",
        "stopOnEntry": true,
        "cwd": "${workspaceRoot}"
      }
    }
  }
}
